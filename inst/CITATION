citHeader("To cite package 'Obsercation' in publications use:")

citEntry(entry = "Misc",
  title        = "Observation: Collect and Process Direct Observation Data",
  author       = personList(as.person("Paul R. Hibbing")),
  year         = "2018",
  url          = "https://github.com/paulhibbing/Observation/",

  textVersion  =
  paste("Paul R. Hibbing (2018).",
        "Observation: Collect and Process Direct Observation Data.",
        "URL https://github.com/paulhibbing/Observation/.")
)